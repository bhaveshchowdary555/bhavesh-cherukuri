num1 = int(input("enter the value of num1 :"))
num2 = int(input("enter the value of num2 :"))
num3 = int(input("enter the value of num3 :"))
if num1>num2 and num1>num3:
    print("num1 is large")
elif num2>num3 and num2>num1:
    print("num2 is large")
elif num2<num3 and num1<num3:
    print("num3 is large")
elif num1==num3 :
    print("all are equal")
